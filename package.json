{
  "name": "lec-integrations-namechecker",
  "version": "1.1.0",
  "private": true,
  "repository": "https://github.com/Lansweeper-public/NameChecker",
  "license": "MIT",
  "scripts": {
    "develop": "env-cmd node src/server.js",
    "debug": "env-cmd node --inspect-brk src/server.js",
    "dev": "next",
    "build": "next build",
    "start:dev": "next start",
    "build:prod": "cross-env NODE_ENV=production next build",
    "start": "cross-env NODE_ENV=production node src/server.js",
    "lint": "eslint \"src/**/*.+(ts|tsx)\" --fix",
    "format": "prettier --write src",
    "release": "semantic-release",
    "test": "env-cmd -f ./.env.test jest --ci --runInBand --forceExit --collectCoverage --detectOpenHandles",
    "test:ci": "jest --ci --runInBand --forceExit --detectOpenHandles",
    "type-check": "tsc watch",
    "cypress:open": "cypress open",
    "cypress:open:dev": "env-cmd cypress open",
    "cypress:run": "cypress run --headed --spec 'cypress/integration/_regression/**/*.feature'",
    "cypress:run:dev": "env-cmd cypress run --headed --spec 'cypress/integration/_regression/**/*.feature'",
    "cypress:install": "cypress install"
  },
  "dependencies": {
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@emotion/core": "^10.1.1",
    "@emotion/styled": "^10.0.27",
    "@sentry/integrations": "^5.15.5",
    "@sentry/node": "^5.15.5",
    "add": "^2.0.6",
    "antd": "^4.18.9",
    "classnames": "^2.2.6",
    "connect-redis": "^4.0.4",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.2",
    "date-fns": "^2.16.1",
    "emotion-theming": "^10.0.27",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "final-form": "^4.20.1",
    "graphql": "^16.3.0",
    "graphql-request": "^4.0.0",
    "is-docker": "^2.0.0",
    "isomorphic-unfetch": "3.0.0",
    "js-cookie": "^2.2.1",
    "less-loader": "^7.3.0",
    "lightship": "^6.2.1",
    "memorystore": "^1.6.6",
    "next": "^12.0.7",
    "next-plugin-antd-less": "^1.6.0",
    "normalize.css": "^8.0.1",
    "pino": "^6.1.1",
    "pino-pretty": "^5.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-final-form": "^6.4.0",
    "react-resize-detector": "^5.2.0",
    "redis": "^3.1.2",
    "sass": "^1.49.9",
    "stoppable": "^1.1.0",
    "uuid": "^8.3.1",
    "yarn": "^1.22.17"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.2",
    "@semantic-release/npm": "^9.0.1",
    "@svgr/webpack": "^6.1.2",
    "@testing-library/cypress": "^8.0.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/classnames": "^2.2.7",
    "@types/express": "^4.17.4",
    "@types/express-session": "^1.17.0",
    "@types/jest": "^27.0.2",
    "@types/js-cookie": "^2.2.5",
    "@types/node": "^13.11.0",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^16.9.4",
    "@types/react-resize-detector": "^5.0.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-emotion": "^10.0.29",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-inline-import": "^3.0.0",
    "conventional-changelog-atom": "^2.0.8",
    "core-js": "^3.6.5",
    "cypress": "^8.3.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-localstorage-commands": "^1.5.0",
    "cypress-xpath": "^1.6.2",
    "env-cmd": "^10.1.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-emotion": "^10.0.27",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "husky": "^3.1.0",
    "jest": "^27.3.1",
    "jest-cli": "^27.3.1",
    "jest-junit": "^13.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "less": "^3.11.1",
    "lint-staged": "^10.2.2",
    "null-loader": "^3.0.0",
    "prettier": "^2.1.2",
    "semantic-release": "^19.0.2",
    "test-listen": "^1.1.0",
    "ts-jest": "^27.0.7",
    "ts-node": "^8.8.2",
    "typescript": "^4.5.2"
  },
  "jest": {
    "testResultsProcessor": "jest-sonar-reporter"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{tsx,ts}": [
      "yarn lint --quiet",
      "git add"
    ],
    "*.{ts,tsx,js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
